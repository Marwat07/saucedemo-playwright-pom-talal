    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "lint": "eslint \"**/*.ts\"",
    "format": "prettier --write \"**/*.ts\""
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "dotenv": "^17.3.1"
  }
}
